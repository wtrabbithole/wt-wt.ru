{
  "root": {
    "type": "big",
    "background-color": "@modalShadeColor",
    "frame": {
      "id": "wnd_frame",
      "size": "1@rw, 1@rh",
      "pos": "50%pw-50%w, 50%ph-50%h",
      "position": "absolute",
      "class": "wndNav",
      "type": "big",
      "frame_header": {
        "activeText": {
          "id": "wnd_title",
          "caption": "yes",
          "text": "#mainmenu/btnBrowser",
          "textHide": "yes"
        },
        "Button_close": {
          "id": "btn_back",
          "style": "_on_click='browserCloseAndUpdateEntitlements'"
        }
      },
      "browser_area": {
        "id": "browser_area",
        "re-type": "9rect",
        "behaviour": "browser",
        "url": "",
        "size": "pw, ph",
        "on_embedded_browser_event": "embedded_browser_event"
      },
      "animated_wait_icon": {
        "id": "browserWaitAnimation",
        "pos": "50%pw-50%w,50%ph-50%h",
        "position": "absolute",
        "background-rotation": "0",
        "behavior": "increment",
        "inc-target": "background-rotation",
        "inc-factor": "120"
      },
      "navBar": {
        "navLeft": {
          "Button_text": {
            "id": "btn_browser_force_external",
            "text": "#browser/open_external",
            "_on_click": "browserForceExternal",
            "btnName": "LB",
            "ButtonImg": []
          }
        },
        "navRight": [
          {
            "Button_text": {
              "id": "btn_browser_back",
              "text": "#mainmenu/btnBack",
              "_on_click": "browserGoBack",
              "btnName": "X",
              "ButtonImg": []
            }
          },
          {
            "Button_text": {
              "id": "btn_browser_reload",
              "text": "#mainmenu/btnRefresh",
              "_on_click": "onBrowserBtnReload",
              "btnName": "Y",
              "ButtonImg": []
            }
          },
          {
            "Button_text": {
              "id": "btn_back",
              "text": "#mainmenu/btnQuit",
              "style": "_on_click='browserCloseAndUpdateEntitlements'",
              "btnName": "B",
              "ButtonImg": []
            }
          }
        ]
      }
    }
  },
  "tdiv": {
    "position": "root",
    "pos": "@bw +1@framePadding, sh-h -1@blockInterval -@bh -1@frameFooterHeight",
    "voiceChatWidget": {
      "value": "{\"viewId\": \"VOICE_CHAT\"}"
    }
  }
}