{
  "tdiv": {
    "id": "chat",
    "position": "relative",
    "flow": "vertical",
    "width": "pw",
    "height": "fh",
    "pos": "1,1",
    "HorizontalListBox": [
      {
        "id": "chat_tabs"
      },
      {
        "width": "pw"
      },
      {
        "class": "tabs"
      },
      {
        "tinyFont": "yes"
      },
      {
        "navigatorShortcuts": "yes"
      },
      {
        "on_select": "onChatTabChange"
      },
      {
        "on_wrap_up": "onWrapUp"
      },
      {
        "on_wrap_down": "onWrapDown"
      },
      {
        "shopFilter": {
          "id": "mpchat",
          "shopFilterText": {
            "text": "#mainmenu/chat"
          }
        }
      },
      {
        "shopFilter": {
          "id": "battlelog",
          "shopFilterText": {
            "text": "#options/_Bttl"
          }
        }
      }
    ],
    "tdiv": {
      "pos": "0, -3px",
      "position": "relative",
      "width": "pw",
      "height": "fh",
      "overflow-y": "auto",
      "auto-scroll": "yes",
      "padding": "0.5*@scrn_tgt/100.0",
      "background-repeat": "expand",
      "background-position": "5",
      "background-image": "#ui/gameuiskin#editbox",
      "background-color": "@editboxBGColor",
      "chatLog": {
        "width": "pw",
        "id": "chat_log",
        "text": "",
        "on_link_click": "onChatLinkClick",
        "on_link_rclick": "onChatLinkRClick"
      }
    },
    "div": [
      {
        "id": "chat_input_back"
      },
      {
        "display": "show"
      },
      {
        "flow": "horizontal"
      },
      {
        "position": "relative"
      },
      {
        "margin-top": "5*@sf/@pf"
      },
      {
        "size": "pw, @buttonHeight"
      },
      {
        "Button_text": {
          "id": "chat_prompt",
          "text": "",
          "width": "15*@scrn_tgt/100.0",
          "top": "50%ph-50%h",
          "position": "relative",
          "style": "padding-left:1*@scrn_tgt/100.0; text-align:left; min-width:15*@scrn_tgt/100.0;",
          "_on_click": "onChatMode",
          "accesskey": "Tab"
        }
      },
      {
        "EditBox": {
          "text": "",
          "id": "chat_input",
          "width": "pw-30.5*@scrn_tgt/100.0",
          "max-len": "250",
          "on_activate": "onChatEntered",
          "on_cancel_edit": "onChatCancel",
          "on_change_value": "onChatChanged",
          "on_wrap_up": "onWrapUp",
          "on_wrap_down": "onWrapDown",
          "mouse-focusable": "yes",
          "margin-right": "0.5*@scrn_tgt/100.0"
        }
      },
      {
        "Button_text": {
          "id": "btn_send",
          "width": "15*@scrn_tgt/100.0",
          "top": "50%ph-50%h",
          "position": "relative",
          "text": "#chat/send",
          "_on_click": "onChatSend",
          "style": "padding-left:1*@scrn_tgt/100.0; text-align:left; min-width:15*@scrn_tgt/100.0;"
        }
      }
    ]
  }
}