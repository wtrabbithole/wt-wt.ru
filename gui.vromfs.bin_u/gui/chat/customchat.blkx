{
  "tdiv": [
    {
      "size": "pw, ph"
    },
    {
      "flow": "vertical"
    },
    {
      "chatBlock": {
        "size": "pw, fh",
        "style": "overflow-y:auto;",
        "overflow-y": "auto",
        "auto-scroll": "yes",
        "chatLog": {
          "id": "menu_chat_text",
          "width": "pw",
          "max-lines": "300",
          "text": "",
          "on_link_click": "onChatLinkClick",
          "on_link_rclick": "onChatLinkRClick"
        }
      }
    },
    {
      "tdiv": {
        "id": "chat_input_place",
        "size": "pw, @buttonHeight",
        "pos": "0, 0.5*@scrn_tgt/100.0",
        "position": "relative",
        "EditBox": {
          "id": "menuchat_input",
          "width": "fw",
          "max-len": "250",
          "selected": "yes",
          "mouse-focusable": "yes",
          "on_activate": "onChatEntered",
          "on_cancel_edit": "onChatCancel",
          "on_wrap_up": "onWrapUp",
          "on_wrap_down": "onWrapDown",
          "font": "@fontSmall",
          "text": "",
          "_customRoomId": "",
          "enable": "no"
        },
        "Button_text": {
          "id": "btn_send",
          "pos": "0.5*@scrn_tgt/100.0, 50%ph-50%h",
          "position": "relative",
          "text": "#chat/send",
          "on_click": "onChatSend",
          "enable": "no",
          "noMargin": "yes",
          "showConsoleImage": "no"
        }
      }
    },
    {
      "dummy": {
        "id": "btn_prevMsg",
        "on_click": "onPrevMsg",
        "behaviour": "accesskey",
        "accessKey": "Alt Up"
      }
    },
    {
      "dummy": {
        "id": "btn_nextMsg",
        "on_click": "onNextMsg",
        "behaviour": "accesskey",
        "accessKey": "Alt Down"
      }
    }
  ]
}