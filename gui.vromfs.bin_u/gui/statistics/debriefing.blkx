                              "on_wrap_left": "onSwitchPlayersTbl",
                              "on_set_focus": "onStatTblFocus",
                              "baseRow": "rows16",
                              "mouse-focusable": "yes"
                            }
                          }
                        }
                      ]
                    }
                  ]
