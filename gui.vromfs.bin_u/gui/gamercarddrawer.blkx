{
  "gamercardDrawer": {
    "id": "gamercard_drawer",
    "position": "root",
    "pos": "0.5sw-0.5w, 0",
    "width": "sw",
    "overflow": "hidden",
    "class": "gamercardDrawer",
    "height": "0",
    "_size-timer": "0",
    "moveOut": "no",
    "on_deactivate": "onDrawerDeactivate",
    "input-transparent": "yes",
    "tdiv": {
      "position": "absolute",
      "height": "sh",
      "width": "sw",
      "pos": "0.5pw-0.5w, ph-sh",
      "flow": "vertical",
      "tdiv": [
        {
          "id": "gamercard_drawer_content"
        },
        {
          "width": "pw"
        },
        {
          "height": "sh - 1@bottomBarHeight - 1@topBarHeight - 0.04@scrn_tgt_font - 2@framePadding"
        },
        {
          "pos": "0.5pw-0.5w, 1@topBarHeight + 0.04@scrn_tgt_font + 2@framePadding"
        },
        {
          "position": "absolute"
        },
        {
          "flow": "vertical"
        },
        {
          "tdiv": {
            "id": "queue_table_container",
            "height": "ph",
            "position": "relative",
            "pos": "0.5pw-0.5w, 0",
            "display": "hide"
          }
        },
        {
          "tdiv": {
            "id": "game_mode_select_container",
            "height": "ph",
            "position": "relative",
            "pos": "0.5pw-0.5w, 0",
            "display": "hide"
          }
        }
      ]
    }
  }
}