[
  {
    "id": "clan_container"
  },
  {
    "padding": "0.01*@scrn_tgt"
  },
  {
    "flow": "vertical"
  },
  {
    "tdiv": [
      {
        "width": "pw"
      },
      {
        "tdiv": [
          {
            "width": "0.53pw"
          },
          {
            "img": {
              "id": "clan-icon",
              "width": "0.26pw",
              "height": "w",
              "max-height": "110",
              "max-width": "110"
            }
          },
          {
            "tdiv": {
              "width": "fw",
              "flow": "vertical",
              "tdiv": [
                {
                  "padding": "0.01*@scrn_tgt, 0"
                },
                {
                  "width": "pw"
                },
                {
                  "flow": "vertical"
                },
                {
                  "tdiv": {
                    "width": "pw",
                    "tdiv": {
                      "id": "nest_lock_clan_req",
                      "clan_locked": "no",
                      "css-hier-invalidate": "yes",
                      "childsActivate": "yes",
                      "Button_text": {
                        "id": "btn_lock_clan_req",
                        "class": "image20",
                        "behaviour": "wrapBroadcast",
                        "tooltip": "#clan/open_close_new_req",
                        "display": "hide",
                        "showConsoleImage": "no",
                        "navigatorShortcuts": "SpaceA",
                        "on_wrap_up": "onWrapUp",
                        "on_wrap_down": "onWrapDown",
                        "on_click": "onLockNewReqests",
                        "img": []
                      },
                      "img": {
                        "id": "img_lock_clan_req",
                        "size": "@unlockStageIconSize, @unlockStageIconSize",
                        "background-color": "@white",
                        "display": "hide",
                        "tooltip": "#clan/closed_for_new_req"
                      }
                    },
                    "textarea": {
                      "id": "clan-title",
                      "style": "color:@activeTextColor; paragraph-indent:0; wrap-indent:0;",
                      "caption": "yes",
                      "text": "",
                      "overflow-y": "none",
                      "pare-text": "yes",
                      "max-width": "@p.pw-@unlockStageIconSize-0.004*@scrn_tgt_font",
                      "max-height": "0.2@scrn_tgt_font"
                    }
                  }
                },
                {
                  "textarea": {
                    "id": "clan-creationDate",
                    "tinyFont": "yes",
                    "style": "paragraph-indent:0; wrap-indent:0;",
                    "text": ""
                  }
                },
                {
                  "textarea": {
                    "id": "clan-region",
                    "tinyFont": "yes",
                    "max-width": "pw",
                    "max-height": "3.3%@scrn_tgt_font",
                    "style": "paragraph-indent:0; wrap-indent:0;",
                    "text": ""
                  }
                },
                {
                  "textarea": {
                    "id": "clan-motto",
                    "max-width": "pw",
                    "max-height": "9.2%@scrn_tgt_font",
                    "overflow-y": "auto",
                    "text": "",
                    "style": "paragraph-indent:0; wrap-indent:0;"
                  }
                },
                {
                  "textarea": {
                    "id": "clan-membershipReqRank",
                    "tinyFont": "yes",
                    "style": "paragraph-indent:0; wrap-indent:0;",
                    "text": ""
                  }
                },
                {
                  "textarea": {
                    "id": "clan-membershipReqBattles",
                    "tinyFont": "yes",
                    "style": "paragraph-indent:0; wrap-indent:0;",
                    "text": ""
                  }
                }
              ]
            }
          },
          {
            "_newline": {
              "size": "0"
            }
          },
          {
            "textarea": {
              "id": "clan-announcement",
              "width": "pw",
              "max-height": "9.2%@scrn_tgt_font",
              "padding": "0.01@scrn_tgt_font",
              "text-align": "left",
              "tinyFont": "yes",
              "overflow-y": "auto",
              "text": ""
            }
          },
          {
            "_newline": {
              "size": "0"
            }
          },
          {
            "textarea": {
              "id": "clan-desc",
              "width": "pw",
              "max-height": "9.2%@scrn_tgt_font",
              "padding": "0.01@scrn_tgt_font",
              "text-align": "left",
              "tinyFont": "yes",
              "overflow-y": "auto",
              "text": ""
            }
          }
        ]
      },
      {
        "tdiv": {
          "width": "fw",
          "flow": "vertical",
          "frameBlock": [
            {
              "width": "pw"
            },
            {
              "padding": "0.009@scrn_tgt_font"
            },
            {
              "flow": "vertical"
            },
            {
              "textareaNoTab": {
                "pos": "0, @mIco - h",
                "position": "relative",
                "text": "#clan/stats"
              }
            },
            {
              "tdiv": [
                {
                  "id": "clan_main_stats"
                },
                {
                  "position": "absolute"
                },
                {
                  "pos": "pw - w, 0"
                },
                {
                  "div": {
                    "tooltip": "#clan/squadron_rating",
                    "cardImg": {
                      "id": "clan_elo_icon",
                      "margin": "0.25@sIco",
                      "background-image": "#ui/gameuiskin#lb_dr_era5",
                      "type": "medium",
                      "total-input-transparent": "yes"
                    },
                    "textarea": {
                      "id": "clan_elo_value",
                      "margin": "0.25@sIco",
                      "text": "",
                      "caption": "yes",
                      "overlayTextColor": "userlog",
                      "removeParagraphIndent": "yes",
                      "input-transparent": "yes"
                    }
                  }
                },
                {
                  "div": {
                    "tooltip": "#clan/squadron_activity",
                    "cardImg": {
                      "id": "clan_activity_icon",
                      "margin": "0.25@sIco",
                      "background-image": "#ui/gameuiskin#lb_activity",
                      "type": "medium",
                      "display": "hide"
                    },
                    "textarea": {
                      "id": "clan_activity_value",
                      "margin": "0.25@sIco",
                      "text": "",
                      "caption": "yes",
                      "overlayTextColor": "userlog",
                      "removeParagraphIndent": "yes",
                      "display": "hide"
                    }
                  }
                }
              ]
            },
            {
              "table": {
                "id": "clan_stats_table",
                "width": "pw",
                "class": "schemeTable"
              }
            },
            {
              "textareaNoTab": {
                "id": "update_stats_info_text",
                "width": "pw",
                "text-align": "center",
                "tinyfont": "yes",
                "overlayTextColor": "faded",
                "text": ""
              }
            }
          ],
          "tdiv": [
            {
              "id": "clan_actions"
            },
            {
              "position": "relative"
            },
            {
              "pos": "pw-w+5, 10"
            },
            {
              "behavior": "posNavigator"
            },
            {
              "navigatorShortcuts": "active"
            },
            {
              "showSelect": "yes"
            },
            {
              "on_activate": "onClanAction"
            },
            {
              "on_wrap_up": "onWrapUp"
            },
            {
              "on_wrap_down": "onWrapDown"
            },
            {
              "Button_text": {
                "id": "btn_showRequests",
                "text": "#clan/btnShowRequests",
                "wink": "no",
                "display": "hide",
                "on_click": "onShowRequests",
                "visualStyle": "secondary",
                "buttonWink": []
              }
            },
            {
              "Button_text": {
                "id": "btn_membership_req",
                "text": "#clan/btn_membership_req",
                "display": "hide",
                "on_click": "onMembershipReq",
                "visualStyle": "secondary",
                "buttonWink": []
              }
            },
            {
              "Button_text": {
                "id": "btn_log",
                "text": "#clan/clan_log",
                "display": "hide",
                "on_click": "onClanLog"
              }
            }
          ],
          "div": {
            "id": "clan_awards_container",
            "position": "relative",
            "pos": "pw-w+5, 10",
            "max-width": "pw",
            "behaviour": "button",
            "on_click": "onClanSeasonRewardLog",
            "display": "hide"
          }
        }
      }
    ]
  },
  {
    "tdiv": {
      "width": "pw",
      "padding": "0, 0.01@scrn_tgt_font",
      "text": {
        "id": "clan-memberCount",
        "text": ""
      },
      "HorizontalListBox": {
        "id": "modes_list",
        "height": "0.04@scrn_tgt",
        "position": "absolute",
        "pos": "pw - w, ph/2 - h/2",
        "on_select": "cp_onStatsModeChange",
        "navigatorShortcuts": "yes",
        "on_wrap_up": "onWrapUp",
        "on_wrap_down": "onWrapDown"
      }
    }
  },
  {
    "table": {
      "id": "clan-membersList",
      "size": "pw, fh",
      "class": "lbTable",
      "behavior": "TableNavigator",
      "text-valign": "center",
      "text-halign": "center",
      "navigatorShortcuts": "yes",
      "selfFocusBorder": "yes",
      "selectOnAttach": "no",
      "on_click": "onSelect",
      "on_r_click": "onUserRClick",
      "on_dbl_click": "onUserCard",
      "on_set_focus": "onMembersListFocus",
      "cur_row": "1",
      "cur_col": "0",
      "num_rows": "-1",
      "overflow-y": "auto",
      "on_wrap_up": "onWrapUp",
      "on_wrap_down": "onWrapDown"
    }
  },
  {
    "animated_wait_icon": {
      "id": "clan_loading",
      "pos": "0.5pw-0.5w, 0.5ph-0.5h",
      "position": "absolute",
      "background-rotation": "0",
      "wait_icon_cock": []
    }
  },
  {
    "navBar": {
      "class": "embedded",
      "navLeft": [
        {
          "Button_text": {
            "id": "btn_complain",
            "text": "#mainmenu/btnComplain",
            "display": "hide",
            "on_click": "onClanComplain",
            "btnName": "LT",
            "ButtonImg": []
          }
        },
        {
          "Button_text": {
            "id": "btn_leaveClan",
            "text": "#clan/btnLeaveClan",
            "display": "hide",
            "on_click": "onLeaveClan",
            "btnName": "LT",
            "ButtonImg": []
          }
        },
        {
          "Button_text": {
            "id": "btn_edit_clan_info",
            "text": "#msgbox/btn_edit",
            "display": "hide",
            "on_click": "onEditClanInfo",
            "btnName": "LB",
            "ButtonImg": []
          }
        },
        {
          "Button_text": {
            "id": "btn_clan_membership_req_edit",
            "text": "#clan/btnChangeMembershipRequirements",
            "display": "hide",
            "_on_click": "onChangeMembershipRequirementsWnd",
            "btnName": "RB",
            "ButtonImg": []
          }
        },
        {
          "Button_text": {
            "id": "btn_showBlacklist",
            "text": "#clan/blacklist",
            "display": "hide",
            "on_click": "onOpenClanBlacklist",
            "btnName": "RT",
            "ButtonImg": []
          }
        },
        {
          "Button_text": {
            "id": "btn_upgrade_clan",
            "text": "#clan/clan_upgrade",
            "display": "hide",
            "on_click": "onUpgradeClan",
            "visualStyle": "secondary",
            "btnName": "R3",
            "buttonWink": [],
            "ButtonImg": []
          }
        },
        {
          "Button_text": {
            "id": "btn_user_options",
            "text": "#mainmenu/btnUserAction",
            "btnName": "Y",
            "_on_click": "onUserOption",
            "ButtonImg": []
          }
        }
      ],
      "navRight": {
        "Button_text": {
          "id": "btn_season_reward_log",
          "text": "#profile/awards",
          "display": "hide",
          "on_click": "onClanSeasonRewardLog",
          "btnName": "X",
          "ButtonImg": []
        }
      }
    }
  }
]