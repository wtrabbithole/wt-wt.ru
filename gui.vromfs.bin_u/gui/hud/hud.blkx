[
  {
    "css": "gui/css.css"
  },
  {
    "tdiv": {
      "id": "hud_obj",
      "position": "absolute",
      "pos": "@bwHud, @bhHud",
      "size": "sw - 2@bwHud, sh - 2@bhHud"
    }
  },
  {
    "textareaNoTab": {
      "id": "unmapped_shortcuts_warning",
      "position": "root",
      "width": "45%sw",
      "pos": "50%sw-50%w, 60%sh-50%h",
      "display": "hide",
      "wink": "no",
      "style": "transp-time:750; color:@warningTextColor; font:@hud_normal; text-align:center;",
      "text": ""
    }
  },
  {
    "textareaNoTab": {
      "id": "server_message",
      "position": "root",
      "width": "45%sw",
      "pos": "50%sw-50%w, 60%sh-50%h",
      "wink": "no",
      "style": "transp-time:750; color:@warningTextColor; font:@hud_normal; text-align:center;",
      "text": "",
      "dummy": {
        "id": "server_message_timer",
        "behavior": "Timer",
        "timer_handler_func": "onServerMessageTimerUpdate",
        "timer_interval_msec": "200"
      }
    }
  },
  {
    "activeText": {
      "id": "time_to_kick_alert_text",
      "text": "",
      "_blink": "no",
      "titleFont": "yes",
      "style": "color:@warningTextColor;",
      "behaviour": "basicFontSize",
      "pos": "50%sw-50%w, 40%sh-50%h",
      "position": "root",
      "font-ht": "28",
      "font-ht-base": "28",
      "font-ht-end": "36",
      "size-time": "200",
      "size-func": "sin",
      "_size-timer": "1"
    }
  },
  {
    "textAreaCentered": {
      "id": "low-quality-model-warning",
      "max-width": "35%sw",
      "pos": "0.5sw-0.5w, @actionBarBottomPos -0.1@shHud -h",
      "position": "root",
      "margin-bottom": "0.01@scrn_tgt_font",
      "style": "color:@warningTextColor; font:@hud_small;",
      "text": "#low_quality_model_in_hangar",
      "display": "hide"
    }
  },
  {
    "dummy": {
      "id": "hud_update",
      "behavior": "Timer",
      "timer_handler_func": "onHudUpdate"
    }
  },
  {
    "hudFade": {
      "id": "hud_fade",
      "position": "root",
      "size": "sw, sh",
      "behavior": "screenFade",
      "input-transparent": "yes"
    }
  }
]