{
  "tdiv": [
    {
      "id": "ww_operation_log_info"
    },
    {
      "width": "pw"
    },
    {
      "height": "fh"
    },
    {
      "position": "relative"
    },
    {
      "flow": "vertical"
    },
    {
      "tdiv": {
        "id": "ww_operation_empty_log",
        "width": "pw",
        "height": "0.04@scrn_tgt",
        "background-color": "@objectiveHeaderBackground",
        "display": "hide",
        "textareaNoTab": {
          "pos": "50%pw-50%w, 50%ph-50%h",
          "position": "relative",
          "text": "#worldwar/log/is_empty"
        }
      }
    },
    {
      "tdiv": [
        {
          "id": "ww_operation_log_frame"
        },
        {
          "width": "pw"
        },
        {
          "height": "fh"
        },
        {
          "flow": "vertical"
        },
        {
          "overflow-y": "auto"
        },
        {
          "tdiv": {
            "id": "show_prev_logs",
            "width": "pw",
            "height": "0.04@scrn_tgt",
            "background-color": "@objectiveHeaderBackground",
            "margin-bottom": "0.01@scrn_tgt",
            "textareaNoTab": {
              "id": "show_prev_logs_text",
              "behavior": "button",
              "width": "fw",
              "padding-left": "1@framePadding",
              "position": "relative",
              "top": "(ph-h)/2",
              "text": "#userlog/showMore",
              "on_click": "onClickShowPrevLogs"
            },
            "Button_text": {
              "id": "show_prev_logs_btn",
              "position": "relative",
              "top": "(ph-h)/2",
              "reduceMinimalWidth": "yes",
              "showConsoleImage": "no",
              "text": "#keysPlus",
              "_on_click": "onClickShowPrevLogs",
              "animated_wait_icon": {
                "id": "show_prev_logs_btn_wait_anim",
                "pos": "50%pw-50%w,50%ph-50%h",
                "position": "absolute",
                "background-rotation": "0"
              }
            }
          }
        },
        {
          "tdiv": {
            "id": "ww_operation_log",
            "width": "pw",
            "flow": "vertical",
            "total-input-transparent": "yes"
          }
        },
        {
          "tdiv": [
            {
              "id": "show_next_logs"
            },
            {
              "width": "pw"
            },
            {
              "height": "0.04@scrn_tgt"
            },
            {
              "background-color": "@objectiveHeaderBackground"
            },
            {
              "textareaNoTab": {
                "id": "show_next_logs_text",
                "behavior": "button",
                "width": "fw",
                "padding-left": "1@framePadding",
                "position": "relative",
                "top": "(ph-h)/2",
                "text": "#userlog/showMore",
                "on_click": "onClickShowNextLogs"
              }
            },
            {
              "Button_text": {
                "id": "show_next_logs_btn",
                "reduceMinimalWidth": "yes",
                "position": "relative",
                "top": "(ph-h)/2",
                "text": "#keysPlus",
                "_on_click": "onClickShowNextLogs",
                "showConsoleImage": "no"
              }
            },
            {
              "Button_text": {
                "id": "show_first_logs_btn",
                "reduceMinimalWidth": "yes",
                "position": "relative",
                "top": "(ph-h)/2",
                "text": "#keysScrollToBottom",
                "_on_click": "onClickShowFirstLogs",
                "showConsoleImage": "no"
              }
            }
          ]
        }
      ]
    },
    {
      "tdiv": {
        "width": "pw",
        "flow-align": "left",
        "background-color": "@operationLogFilterBar",
        "Button_text": {
          "id": "ww_log_filters",
          "visualStyle": "header",
          "_on_click": "onShowOperationLogFilters",
          "text": "#worldwar/logFilters"
        },
        "textareaNoTab": {
          "id": "hidden_logs_text",
          "padding-left": "1@framePadding",
          "position": "relative",
          "top": "(ph-h)/2",
          "smallFont": "yes",
          "text": "#userlog/showMore"
        }
      }
    }
  ]
}