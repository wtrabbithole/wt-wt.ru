{
  "tdiv": [
    {
      "id": "airfield_object"
    },
    {
      "size": "pw, fh"
    },
    {
      "flow": "vertical"
    },
    {
      "tdiv": {
        "id": "airfields_list",
        "pos": "50%pw-50%w, 0",
        "position": "relative",
        "flow": "h-flow"
      }
    },
    {
      "tdiv": [
        {
          "id": "airfield_block"
        },
        {
          "size": "pw, fh"
        },
        {
          "flow": "vertical"
        },
        {
          "display": "hide"
        },
        {
          "textareaNoTab": {
            "id": "airfield_info_text",
            "width": "pw",
            "background-color": "@objectiveHeaderBackground",
            "padding": "0, 1@framePadding",
            "margin-bottom": "1@dp",
            "smallFont": "yes",
            "text-align": "center",
            "text": ""
          }
        },
        {
          "tdiv": {
            "size": "pw, 1@mIco+2@framePadding",
            "background-color": "@objectiveHeaderBackground",
            "tdiv": {
              "height": "ph",
              "left": "50%pw-50%w",
              "position": "relative",
              "textareaNoTab": {
                "id": "free_formations_text",
                "top": "50%ph-50%h",
                "position": "relative",
                "text-align": "right",
                "text": ""
              },
              "FormationRadioButtonsList": {
                "id": "free_formations",
                "behavior": "Timer",
                "top": "50%ph-50%h",
                "position": "relative"
              }
            }
          }
        },
        {
          "tdiv": {
            "size": "pw, fh",
            "overflow-y": "auto",
            "scrollbarShortcuts": "yes",
            "padding-left": "1@framePadding",
            "FormationRadioButtonsList": {
              "id": "cooldowns_list",
              "behavior": "Timer",
              "pos": "0, 0.01@scrn_tgt",
              "position": "relative",
              "width": "pw",
              "flow": "h-flow",
              "flow-align": "left"
            }
          }
        }
      ]
    }
  ]
}