{
  "slotsHeader": {
    "id": "slots_header_",
    "top": "50%ph-50%h",
    "position": "relative",
    "img": {
      "id": "hdr_image",
      "background-image": "#ui/gameuiskin#country_0"
    },
    "bonusNoFrame": {
      "id": "hdr_bonus"
    },
    "textareaNoTab": {
      "id": "hdr_caption",
      "pos": "50%pw-50%w, pw-2*@sf/@pf",
      "position": "absolute",
      "hideEmptyText": "yes",
      "text-align": "center",
      "text": ""
    }
  },
  "slotsScrollDiv": {
    "height": "@slotbarHeight+10*@scrn_tgt/100.0",
    "pos": "0, -4*@scrn_tgt/100.0-4",
    "position": "relative",
    "input-transparent": "yes",
    "overflow-x": "auto",
    "table": {
      "id": "airs_table",
      "pos": "0, 0.5@slotbarHeight+5*@scrn_tgt/100.0 -0.53h",
      "position": "relative",
      "class": "slotbarTable",
      "navigatorShortcuts": "yes",
      "behavior": "columnNavigator",
      "cur_col": "-1",
      "cur_row": "-1",
      "fixed_row": "0",
      "on_select": "onSlotbarSelect",
      "_on_dbl_click": "onSlotbarDblClick",
      "on_wrap_up": "onWrapUp",
      "on_wrap_down": "onWrapDown",
      "clearOnFocusLost": "no"
    }
  }
}