[
  {
    "dummy": {
      "id": "btn_slotbar_prev_country",
      "on_click": "prevCountry",
      "behaviour": "accesskey",
      "accessKey": "J:LB"
    }
  },
  {
    "dummy": {
      "id": "btn_slotbar_next_country",
      "on_click": "nextCountry",
      "behaviour": "accesskey",
      "accessKey": "J:RB"
    }
  },
  {
    "tdiv": {
      "id": "slotbar_background",
      "size": "1@slotbarWidthFull, 1@slotbarHeight + 1@slotbarOffset",
      "display": "hide",
      "pos": "50%pw-50%w, 0",
      "position": "absolute",
      "slotbarEdge": [
        {
          "size": "1@slotbarWidthFull, 1@slotbarHeight"
        },
        {
          "pos": "0,0"
        },
        {
          "position": "absolute"
        },
        {
          "navConsoleBlock": {
            "id": "slotbar_nav_block_left",
            "pos": "-w+10, 50%ph-50%h",
            "position": "absolute",
            "height": "0.04@scrn_tgt",
            "display": "hide",
            "directionImage": {
              "pos": "0, 50%ph-50%h",
              "position": "relative",
              "size": "0.02@scrn_tgt, ph",
              "rotation": "180"
            },
            "buttonImage": {
              "pos": "0, 50%ph-50%h",
              "position": "relative",
              "btnName": "LT",
              "size": "ph,ph"
            }
          }
        },
        {
          "navConsoleBlock": {
            "id": "slotbar_nav_block_right",
            "pos": "pw-10, 50%ph-50%h",
            "position": "absolute",
            "height": "0.04@scrn_tgt",
            "display": "hide",
            "buttonImage": {
              "btnName": "RT",
              "pos": "0, 50%ph-50%h",
              "position": "relative",
              "size": "ph,ph"
            },
            "directionImage": {
              "pos": "0, 50%ph-50%h",
              "position": "relative",
              "size": "0.02@scrn_tgt, ph"
            }
          }
        }
      ],
      "slotbarTop": {
        "id": "slotbar_buttons_place",
        "size": "pw, 1@slotbarTop",
        "pos": "0,-h",
        "position": "absolute",
        "display": "hide",
        "behavior": "Timer",
        "tdiv": [
          {
            "id": "autorefill-settings"
          },
          {
            "pos": "0, 50%ph - 50%h"
          },
          {
            "padding-left": "3@dp"
          },
          {
            "position": "absolute"
          },
          {
            "width": "pw"
          },
          {
            "display": "hide"
          },
          {
            "behavior": "wrapNavigator"
          },
          {
            "navigatorShortcuts": "yes"
          },
          {
            "childsActivate": "yes"
          },
          {
            "on_wrap_up": "onWrapUp"
          },
          {
            "on_wrap_down": "onWrapDown"
          },
          {
            "CheckBox": {
              "id": "slots-autorepair",
              "class": "with_image",
              "tooltip": "#shop/auto_repair",
              "on_change_value": "onSlotsChangeAutoRefill",
              "margin-right": "h",
              "infoImg": {
                "background-image": "#ui/gameuiskin#slot_repair"
              },
              "CheckBoxImg": []
            }
          },
          {
            "CheckBox": {
              "id": "slots-autoweapon",
              "class": "with_image",
              "tooltip": "#shop/auto_buy_weapons",
              "on_change_value": "onSlotsChangeAutoRefill",
              "margin-right": "h",
              "infoImg": {
                "background-image": "#ui/gameuiskin#slot_weapons"
              },
              "CheckBoxImg": []
            }
          }
        ]
      }
    }
  },
  {
    "HorizontalListBox": {
      "id": "slotbar-countries",
      "pos": "50%pw-50%w, 0",
      "position": "relative",
      "useShortcuts": "no",
      "class": "slotbar",
      "on_select": "onSlotbarCountry"
    }
  },
  {
    "animShow": {
      "id": "slotbar_shade",
      "size": "1@slotbarWidthFull, 1@slotbarHeight + 1@slotbarOffset + 1@slotbarTop",
      "pos": "50%pw-50%w, -1@slotbarTop",
      "position": "absolute",
      "background-color": "@shadeBackgroundColor",
      "animation": "hide",
      "color-factor": "0"
    }
  },
  {
    "dummy": {
      "id": "slotbar_timer",
      "behavior": "Timer"
    }
  }
]