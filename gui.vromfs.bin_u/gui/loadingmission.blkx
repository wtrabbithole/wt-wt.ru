{
  "css": "gui/css.css",
  "root": [
    {
      "img": {
        "size": "pw+4,ph+4",
        "pos": "-2,-2",
        "position": "absolute",
        "name": "IMAGE"
      }
    },
    {
      "tdiv": {
        "size": "sw+4, sh+4",
        "pos": "-2,-2",
        "position": "root",
        "background-color": "@shadeBackgroundColor3"
      }
    },
    {
      "tdiv": {
        "size": "sw+4, 18%sh+1",
        "pos": "-2,-2",
        "position": "root",
        "background-color": "@shadeBackgroundColor",
        "border": "yes",
        "border-color": "@commonMenuButtonColor"
      }
    },
    {
      "tdiv": {
        "size": "sw+4, 18%sh+1",
        "pos": "-2,sh-h+4",
        "position": "root",
        "background-color": "@shadeBackgroundColor",
        "border": "yes",
        "border-color": "@commonMenuButtonColor"
      }
    },
    {
      "tdiv": {
        "name": "LOADANIM",
        "position": "absolute",
        "pos": "pw - w - @bw, 85.5%sh + 5",
        "animated_wait_icon": {
          "pos": "0, 50%h-50%h",
          "position": "absolute",
          "background-rotation": "0",
          "wait_icon_cock": []
        },
        "loading": {
          "name": "loading",
          "pos": "48*@sf/@pf,(ph-h)/2",
          "position": "relative",
          "re-type": "text",
          "style": "font:@big;color:@mainTitleTextColor;",
          "text": "#loading",
          "text-align": "left"
        }
      }
    },
    {
      "titletext": {
        "name": "HEADER"
      }
    },
    {
      "textarea": {
        "name": "TEXT",
        "behaviour": "textArea",
        "re-type": "textarea",
        "style": "color:@activeTextColor;",
        "min-width": "0.75sw",
        "max-width": "0.75sw",
        "pos": "12.5%sw,20%sh",
        "text-align": "left",
        "position": "absolute",
        "height": "60%sh",
        "auto-scroll": "yes",
        "scrollType": "hidden",
        "focus": "yes"
      }
    },
    {
      "dummy": {
        "id": "cutscene_update",
        "behavior": "Timer",
        "timer_handler_func": "onUpdate"
      }
    }
  ],
  "navBar": {
    "textarea": {
      "name": "HINT",
      "style": "font:@bold;",
      "width": "pw - 0.35sh",
      "text-align": "left"
    },
    "textAreaCentered": {
      "id": "help_text",
      "text": "",
      "style": "color:@helpF1Color;",
      "pos": "(pw-w)/2, -h",
      "position": "absolute",
      "width": "pw"
    },
    "navRight": {
      "Button_text": {
        "id": "btn_select",
        "name": "btn_select",
        "text": "#options/select",
        "btnName": "A",
        "_on_click": "onApply",
        "ButtonImg": []
      }
    }
  }
}