                      {
                        "max": "100"
                      },
                      {
                        "on_change_value": "onConvertChanged"
                      },
                      {
                        "behaviour": "wrapNavigator"
                      },
                      {
                        "navigatorShortcuts": "yes"
                      },
                      {
                        "on_wrap_up": "onWrapUp"
