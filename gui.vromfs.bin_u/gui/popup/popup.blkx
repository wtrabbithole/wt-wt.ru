{
  "width": "pw",
  "overflow": "hidden",
  "_size-timer": "0",
  "on_click": "onClickPopup",
  "on_r_click": "onRClickPopup",
  "timer_handler_func": "onTimerUpdate",
  "timer_interval_msec": "10000",
  "popupWrap": {
    "id": "popup_wrap",
    "padding": "5",
    "class": "tinyFont",
    "background-position": "4",
    "background-repeat": "expand",
    "background-color": "@white",
    "background-image": "#ui/gameuiskin#frame_opaq",
    "position": "absolute",
    "size": "p.p.w, p.p.h",
    "bottom": "0",
    "left": "0",
    "flow": "vertical",
    "input-transparent": "yes",
    "closeBtn": {
      "id": "close_button",
      "pos": "pw-w, 5",
      "position": "absolute",
      "on_click": "onClosePopup",
      "img": []
    },
    "activeText": {
      "id": "title",
      "tinyFont": "yes",
      "position": "relative",
      "text": "",
      "input-transparent": "yes"
    },
    "textAreaNoScroll": {
      "id": "msg",
      "text": "",
      "input-transparent": "yes",
      "pare-text": "yes"
    },
    "tdiv": {
      "id": "popup_buttons_place",
      "max-width": "pw",
      "pos": "pw-w, 0",
      "position": "relative"
    }
  }
}