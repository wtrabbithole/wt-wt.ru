{
  "debriefingTab": {
    "id": "my_stats_tab",
    "flow": "vertical",
    "debrBlock": [
      {
        "size": "pw, 0.12@scrn_tgt"
      },
      {
        "pos": "0, 1@debrMyPlaceBlockOffset"
      },
      {
        "position": "relative"
      },
      {
        "padding": "1@debrPad"
      },
      {
        "background-color": "@debrBgShadeMyPlaceBlock"
      },
      {
        "tdiv": [
          {
            "id": "my_place_move_box"
          },
          {
            "height": "0.036@sf"
          },
          {
            "pos": "1.2@debrPad, ph/2-h/2"
          },
          {
            "position": "absolute"
          },
          {
            "css-hier-invalidate": "yes"
          },
          {
            "total-input-transparent": "yes"
          },
          {
            "display": "hide"
          },
          {
            "activeText": {
              "id": "my_place_label",
              "fontNormal": "yes",
              "animateFontSize": "yes",
              "text": "#debriefing/placeInMyTeam"
            }
          },
          {
            "activeText": {
              "id": "my_place_in_mptable",
              "fontNormal": "yes",
              "animateFontSize": "yes",
              "overlayTextColor": "userlog",
              "text": "0"
            }
          }
        ]
      },
      {
        "tdiv": {
          "id": "wnd_total",
          "pos": "67%pw -w, ph/2-h/2",
          "position": "absolute",
          "flow": "horizontal"
        }
      },
      {
        "Button_text": {
          "id": "btn_buy_premium_award",
          "pos": "78%pw-w/2, ph/2-h/2",
          "position": "absolute",
          "class": "buyPremAward",
          "on_click": "onBuyPremiumAward",
          "visualStyle": "purchase",
          "tooltip": "",
          "display": "hide",
          "btnName": "Y",
          "ButtonImg": [],
          "textarea": {
            "id": "label",
            "pos": "0, ph/2-h/2",
            "position": "relative",
            "min-width": "pw",
            "input-transparent": "yes",
            "text-align": "center",
            "overlayTextColor": "premiumNotEarned",
            "text": ""
          }
        }
      },
      {
        "tdiv": {
          "id": "active_wager_container",
          "size": "1@debrWagerHeight, 1@debrWagerHeight",
          "pos": "pw -1@debrBonusWidth -2@debrPad -85%w, ph/2-h/2",
          "position": "absolute",
          "title": "$tooltipObj",
          "tdiv": {
            "itemIconBlock": {
              "display": "hide",
              "id": "active_wager_icon",
              "debriefingSize": "yes",
              "overflow": "hidden"
            },
            "img": {
              "id": "active_wager_result_icon",
              "position": "absolute",
              "pos": "pw-1.2w, ph-1.2h",
              "size": "@unlockIconSize, @unlockIconSize",
              "background-image": "#ui/gameuiskin#favorite",
              "display": "hide"
            }
          },
          "textareaNoTab": {
            "id": "active_wager_text",
            "text": ""
          },
          "tooltipObj": {
            "id": "active_wager_tooltip",
            "on_tooltip_open": "onWagerTooltipOpen",
            "on_tooltip_close": "onTooltipObjClose",
            "display": "hide",
            "enable": "no"
          }
        }
      },
      {
        "debriefingBonusIconNest": {
          "id": "bonus_ico_place",
          "size": "1@debrBonusWidth, 1@debrBonusHeight",
          "pos": "pw -1@debrPad -w, ph/2-h/2",
          "position": "absolute",
          "img": {
            "id": "bonus_ico",
            "size": "pw, ph",
            "display": "hide"
          }
        }
      },
      {
        "tdiv": {
          "id": "start_bonus_place",
          "size": "2.5@debrBonusWidth, 2.5@debrBonusHeight",
          "pos": "pw -1@debrPad -50%@debrBonusWidth -50%w, ph/2-h/2",
          "position": "absolute"
        }
      }
    ],
    "tdiv": {
      "size": "pw, fh",
      "debrBlock": {
        "id": "my_stats_awards_block",
        "size": "2@debriefingUnlockIconSize +3@debrPad, ph",
        "class": "button",
        "behaviour": "button",
        "on_click": "onViewAwards",
        "tdiv": [
          {
            "height": "ph"
          },
          {
            "pos": "pw/2-w/2, 0"
          },
          {
            "position": "relative"
          },
          {
            "tdiv": {
              "id": "awards_list_unlocks",
              "width": "1@debriefingUnlockIconSize",
              "height": "ph",
              "margin": "1@debrPad",
              "overflow": "visible",
              "flow": "vertical",
              "display": "hide"
            }
          },
          {
            "tdiv": {
              "id": "awards_list_streaks",
              "width": "1@debriefingUnlockIconSize",
              "height": "ph",
              "margin": "1@debrPad",
              "overflow": "visible",
              "flow": "vertical",
              "display": "hide"
            }
          }
        ],
        "debrSeparator": {
          "class": "right"
        }
      },
      "tdiv": [
        {
          "size": "fw, ph"
        },
        {
          "flow": "vertical"
        },
        {
          "debrBlock": [
            {
              "id": "pve_trophy_block"
            },
            {
              "size": "pw, 0.75@itemHeight"
            },
            {
              "padding": "1@debrPad"
            },
            {
              "display": "hide"
            },
            {
              "tdiv": {
                "id": "pve_trophy_progress",
                "pos": "0, ph/2-h/2",
                "position": "relative",
                "width": "75%pw"
              }
            },
            {
              "tdiv": {
                "id": "pve_trophy_chest",
                "size": "0.75@itemHeight, 0.75@itemHeight",
                "pos": "pw-w, ph/2-h/2",
                "position": "absolute"
              }
            },
            {
              "tdiv": {
                "size": "0.75@itemHeight, 0.75@itemHeight",
                "pos": "pw-w-0.25w, ph/2-h/2",
                "position": "absolute",
                "tdiv": {
                  "id": "pve_trophy_content",
                  "size": "1@itemHeight, 1@itemHeight",
                  "pos": "pw/2-w/2, ph/2-h/2",
                  "position": "absolute"
                }
              }
            },
            {
              "activeText": {
                "id": "pve_award_already_received",
                "pos": "pw-w- 1@debrPad, 50%ph",
                "position": "absolute",
                "text": "#debriefing/awardHasAlreadyBeenReceived",
                "display": "hide"
              }
            },
            {
              "debrSeparator": {
                "class": "bottom"
              }
            }
          ]
        },
        {
          "debrBlock": {
            "id": "no_awards_block",
            "size": "pw, 0.12@scrn_tgt",
            "padding": "1@debrPad",
            "display": "hide",
            "textarea": {
              "id": "no_awards_caption",
              "width": "pw",
              "pos": "0, ph/2-h/2",
              "position": "absolute",
              "overlayTextColor": "userlog",
              "text": ""
            },
            "debrSeparator": {
              "class": "bottom"
            }
          }
        },
        {
          "tdiv": [
            {
              "size": "pw, fh"
            },
            {
              "debrBlock": {
                "id": "my_stats_scroll_block",
                "size": "fw, ph",
                "flow": "vertical",
                "overflow-y": "auto",
                "scrollbarShortcuts": "yes",
                "css-hier-invalidate": "yes",
                "tdiv": {
                  "width": "pw",
                  "min-height": "ph",
                  "padding": "1@debrPad",
                  "flow": "vertical",
                  "table": {
                    "id": "stat_table",
                    "width": "pw",
                    "baseRow": "rows16",
                    "flow": "vertical",
                    "total-input-transparent": "yes"
                  },
                  "tdiv": {
                    "id": "leaderbord_stats",
                    "width": "pw",
                    "pos": "0, 1@debrPad",
                    "position": "relative",
                    "flow": "h-flow",
                    "display": "hide"
                  },
                  "textareaNoTab": {
                    "id": "stat_info_text",
                    "width": "pw",
                    "pos": "0, 3@debrPad",
                    "position": "relative",
                    "overlayTextColor": "",
                    "text-align": "right",
                    "text": "",
                    "display": "hide"
                  }
                }
              }
            },
            {
              "debrBlock": {
                "id": "researches_block",
                "size": "2@slot_width +2@debrPad +1@scrollBarSize, ph",
                "debrSeparator": {
                  "class": "left"
                },
                "tdiv": {
                  "id": "researches_scroll_block",
                  "size": "pw, ph",
                  "overflow-y": "auto",
                  "scrollbarShortcuts": "left",
                  "tdiv": [
                    {
                      "width": "pw"
                    },
                    {
                      "min-height": "ph"
                    },
                    {
                      "padding": "1@debrPad"
                    },
                    {
                      "flow": "vertical"
                    },
                    {
                      "activeText": {
                        "text": "#debriefing/researched_unit"
                      }
                    },
                    {
                      "tdiv": [
                        {
                          "width": "pw"
                        },
                        {
                          "textAreaCentered": {
                            "id": "no_air_text",
                            "width": "pw",
                            "pos": "pw/2-w/2, ph/2-h/2",
                            "position": "absolute",
                            "tinyFont": "yes",
                            "text": ""
                          }
                        },
                        {
                          "textAreaCentered": {
                            "id": "air_item_text",
                            "width": "pw",
                            "pos": "pw/2-w/2, ph/2-h/2",
                            "position": "absolute",
                            "tinyFont": "yes",
                            "text": ""
                          }
                        },
                        {
                          "tdiv": {
                            "id": "air_item_place",
                            "width": "pw",
                            "min-height": "@slot_height + 2@slot_vert_pad",
                            "class": "rankUpList",
                            "flow": "h-flow"
                          }
                        }
                      ]
                    },
                    {
                      "activeText": {
                        "pos": "0, 0.67@debrPad",
                        "position": "relative",
                        "text": "#debriefing/research_list"
                      }
                    },
                    {
                      "tdiv": {
                        "id": "research_list",
                        "width": "pw",
                        "flow": "vertical",
                        "textAreaCentered": {
                          "id": "research_list_text",
                          "width": "pw",
                          "pos": "pw/2-w/2, (@slot_height + 2@slot_vert_pad)/2-h/2",
                          "position": "absolute",
                          "tinyFont": "yes",
                          "text": ""
                        },
                        "tdiv": {
                          "id": "modifications_objects_place",
                          "width": "pw",
                          "min-height": "1@slot_height + 2@slot_vert_pad",
                          "flow": "vertical"
                        }
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      ]
    }
  }
}