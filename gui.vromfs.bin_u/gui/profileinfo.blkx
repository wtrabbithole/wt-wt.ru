[
  {
    "tdiv": [
      {
        "size": "pw, fh"
      },
      {
        "tdiv": [
          {
            "flow": "vertical"
          },
          {
            "size": "0.45@scrn_tgt, ph"
          },
          {
            "pos": "0, 0"
          },
          {
            "position": "relative"
          },
          {
            "frameBlock": {
              "width": "pw",
              "pos": "50%pw-50%w, 0",
              "position": "relative",
              "padding": "1@blockInterval",
              "Button_text": {
                "id": "btn_profile_icon",
                "_on_click": "onChangePilotIcon",
                "class": "profileImg",
                "img": {
                  "id": "profile-icon",
                  "size": "pw,ph",
                  "background-image": "#ui/images/avatars/cardicon_default"
                },
                "ButtonImg": {
                  "style": "pos:pw - w - 4@dp, ph - h - 4@dp; height:@buttonIconHeight;"
                }
              },
              "tdiv": [
                {
                  "height": "ph"
                },
                {
                  "pos": "0, 50%ph-50%h"
                },
                {
                  "position": "relative"
                },
                {
                  "padding": "1@blockInterval"
                },
                {
                  "flow": "vertical"
                },
                {
                  "width": "fw"
                },
                {
                  "textareaNoTab": {
                    "id": "profile-clanTag",
                    "max-width": "pw",
                    "tooltip": ""
                  }
                },
                {
                  "activeText": {
                    "id": "profile-name",
                    "talign": "left",
                    "style": "color:@mainPlayerColor"
                  }
                },
                {
                  "textareaNoTab": {
                    "id": "profile-currentUser-title",
                    "class": "highlightButton",
                    "smallFont": "yes",
                    "on_click": "getNewTitles",
                    "margin-top": "3@dp",
                    "max-width": "pw",
                    "hideEmptyText": "yes",
                    "text": "",
                    "css-hier-invalidate": "yes",
                    "ButtonImg": {
                      "style": "top:50%ph-50%h;"
                    }
                  }
                },
                {
                  "textareaNoTab": {
                    "id": "profile-currentUser-steamName",
                    "width": "pw",
                    "smallFont": "yes"
                  }
                },
                {
                  "textareaNoTab": {
                    "id": "profile-currentUser-facebookName",
                    "width": "pw",
                    "smallFont": "yes"
                  }
                }
              ]
            }
          },
          {
            "frameBlock": [
              {
                "height": "fh"
              },
              {
                "width": "pw"
              },
              {
                "padding": "1@blockInterval"
              },
              {
                "flow": "vertical"
              },
              {
                "tdiv": [
                  {
                    "halign": "center"
                  },
                  {
                    "activeText": {
                      "text": "#mainmenu/rank"
                    }
                  },
                  {
                    "activeText": {
                      "id": "profile-rank",
                      "text": "1",
                      "padding-left": "1@blockInterval"
                    }
                  },
                  {
                    "cardImg": {
                      "id": "profile-prestige",
                      "padding-left": "1@blockInterval"
                    }
                  }
                ]
              },
              {
                "activeText": {
                  "id": "profile-prestige_title",
                  "text": "",
                  "halign": "center"
                }
              },
              {
                "activeText": {
                  "id": "profile-exp",
                  "text": " ",
                  "halign": "center"
                }
              },
              {
                "expProgress": {
                  "id": "profile-rankProgress",
                  "width": "0.2@scrn_tgt",
                  "position": "relative",
                  "pos": "50%pw-50%w, 0",
                  "margin-top": "1@blockInterval",
                  "margin-bottom": "0.02@scrn_tgt",
                  "value": "0"
                }
              },
              {
                "tdiv": {
                  "id": "country_stats_nest",
                  "width": "75%pw",
                  "pos": "50%pw-50%w, 0",
                  "position": "relative"
                }
              }
            ]
          }
        ]
      },
      {
        "tdiv": {
          "id": "stats-container",
          "size": "fw, ph",
          "padding-left": "1@blockInterval",
          "flow": "vertical",
          "HorizontalListBox": {
            "id": "modes_list",
            "position": "relative",
            "pos": "0, 0",
            "navigatorShortcuts": "yes",
            "on_select": "onProfileStatsModeChange",
            "on_wrap_up": "onWrapUp",
            "on_wrap_down": "onWrapDown",
            "focus": "yes"
          },
          "frameBlock": {
            "size": "pw, fh",
            "position": "relative",
            "pos": "0, 1@blockInterval",
            "padding": "1@blockInterval",
            "overflow-y": "auto",
            "scrollbarShortcuts": "yes",
            "table": {
              "id": "stats_table",
              "class": "lbTable",
              "text-halign": "center",
              "width": "pw"
            }
          }
        }
      }
    ]
  },
  {
    "tdiv": {
      "width": "pw",
      "pos": "0, 0",
      "position": "relative",
      "padding-top": "1@blockInterval",
      "flow": "vertical",
      "tdiv": {
        "id": "leaderboards_stats_row",
        "width": "pw",
        "margin-bottom": "1@blockInterval",
        "behaviour": "wrapNavigator",
        "navigatorShortcuts": "yes",
        "childsActivate": "yes",
        "on_wrap_up": "onWrapUp",
        "on_wrap_down": "onWrapDown",
        "ComboBox": {
          "id": "leaderboard_modes_list",
          "position": "relative",
          "pos": "0, 50%ph-50%h",
          "width": "0.3@sf",
          "display": "hide",
          "enable": "no",
          "focusFrame": "yes",
          "on_select": "onLbModeSelect"
        },
        "CheckBox": {
          "id": "stats_type_profile",
          "display": "hide",
          "pos": "pw-w, 50%ph-50%h",
          "position": "absolute",
          "text": "#mainmenu/btnMonthLb",
          "on_change_value": "onStatsTypeChange",
          "value": "yes",
          "CheckBoxImg": []
        }
      },
      "frameBlock": {
        "width": "pw",
        "table": {
          "id": "profile_leaderboard",
          "width": "pw",
          "class": "lbTable",
          "text-halign": "center"
        }
      }
    }
  },
  {
    "animated_wait_icon": {
      "id": "profile_loading",
      "pos": "0.5(pw-w),0.4(ph-h)",
      "position": "absolute",
      "background-rotation": "0",
      "wait_icon_cock": []
    }
  },
  {
    "dummy": {
      "id": "profile_update",
      "behavior": "Timer",
      "timer_handler_func": "onUpdate"
    }
  }
]