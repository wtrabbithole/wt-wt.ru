<<#level>>
warbondShopLevel {
  id:t='<<id>>'
  top:t='50%ph-50%h'
  left:t='<<posX>>'
  position:t='absolute'
  tooltip:t='<<tooltip>>'
  status = <<status>>

  textareaNoTab {
    id:t='<<id>>_text'
    pos:t='0, 50%ph-50%h'
    position:t='relative'
    hideEmptyText:t='yes'
    tinyFont:t='yes'
    width:t='pw'
    text-align:t='center'
    text:t='<<text>>'
  }
}
<</level>>