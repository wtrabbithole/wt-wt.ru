{
  "tdiv": {
    "id": "animated_bg",
    "pos": "50%sw-50%w,50%sh-50%h",
    "position": "root",
    "width": "2.5h",
    "height": "sh",
    "behaviour": "multiLayerImage",
    "blend-time": "500"
  },
  "div": [
    {
      "width": "pw"
    },
    {
      "position": "absolute"
    },
    {
      "pos": "0, 50%ph-50%h"
    },
    {
      "position": "relative"
    },
    {
      "re-type": "9rect"
    },
    {
      "background-color": "@white"
    },
    {
      "background-position": "1,5"
    },
    {
      "background-repeat": "expand"
    },
    {
      "background-image": "#ui/gameuiskin#optionlist_background_middle"
    },
    {
      "tdiv": [
        {
          "flow": "vertical"
        },
        {
          "width": "pw"
        },
        {
          "height": "ph"
        },
        {
          "position": "relative"
        },
        {
          "padding": "15"
        },
        {
          "textAreaCentered": {
            "id": "msgText",
            "width": "1.1@scrn_tgt_font",
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "fontNormal": "yes",
            "margin": "1.5*@scrn_tgt/100.0"
          }
        },
        {
          "everyDayRewardProgress": {
            "id": "loading_progress_box",
            "position": "relative",
            "pos": "50%pw-50%w",
            "width": "0.5@sf",
            "foreground-color": "@red",
            "pattern": {
              "type": "dark_diag_lines"
            }
          }
        },
        {
          "textAreaCentered": {
            "id": "msgTextSub",
            "width": "1.1@scrn_tgt_font",
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "tinyFont": "yes",
            "margin": "0.5*@scrn_tgt/100.0"
          }
        },
        {
          "div": {
            "id": "buttons_holder",
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "margin": "@scrn_tgt/100.0"
          }
        }
      ]
    }
  ],
  "BottomMenuPanel": {
    "pattern": {
      "type": "bright_texture"
    },
    "Red_shadow_light": {
      "pos": "50%pw-50%w, 2",
      "position": "absolute",
      "size": "25%pw, ph"
    },
    "GameLogoPlate": {
      "pos": "50%sw-50%w, ph-h",
      "position": "absolute",
      "Shadow": [],
      "redPlate": {
        "pattern": {
          "type": "dark_diag_lines"
        }
      },
      "tdiv": {
        "pos": "50%pw-50%w, 0.5(ph-1@bh) - 50%h",
        "position": "absolute",
        "animated_wait_icon": {
          "id": "rot",
          "pos": "-0.1w-1, 50%ph-50%h",
          "position": "relative",
          "background-rotation": "0",
          "wait_icon_cock": []
        },
        "textAreaCentered": {
          "pos": "5, 50%ph-50%h-2",
          "position": "relative",
          "overlayTextColor": "active",
          "textShade": "yes",
          "text": "#loading"
        }
      }
    },
    "copyright": {
      "pos": "pw-w-1@bw, 0.5(ph-1@bh) - 50%h",
      "position": "absolute"
    }
  },
  "dummy": {
    "id": "updater_timer",
    "behavior": "Timer",
    "timer_handler_func": "onUpdate"
  }
}