                    "textarea": {
                      "id": "session_battleRating",
                      "smallFont": "yes"
                    }
                  }
                },
                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "session_environment",
                      "smallFont": "yes"
                    }
                  }
                },
                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "session_laps",
                      "smallFont": "yes"
