              "pos": "50%pw-50%w, 0",
              "display": "hide",
              "enable": "no",
              "behaviour": "wrapNavigator",
              "navigatorShortcuts": "yes",
              "childsActivate": "yes",
              "on_wrap_up": "onWrapUp",
