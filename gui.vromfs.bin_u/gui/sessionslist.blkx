      {
        "textarea": {
          "text": "#mainmenu/skirmish/noProgress",
          "pos": "50%pw-50%w, 0.01@scrn_tgt",
          "position": "relative",
          "max-width": "pw",
          "removeParagraphIndent": "yes",
          "overlayTextColor": "warning"
        }
      }
    ],
    "framedMessage": {
      "id": "info-text",
      "pos": "50%pw-50%w, 50%ph-50%h",
      "position": "absolute",
      "max-width": "pw-2@bw-0.02@scrn_tgt",
      "display": "hide"
    },
