              {
                "padding-left": "2%pw"
              },
              {
                "flow": "vertical"
              },
              {
                "table": {
                  "id": "sessions-header",
                  "width": "pw",
                  "text-valign": "center",
                  "tr": [
                    {
                      "size": "pw, @baseTrHeight"
                    },
                    {
                      "pos": "0, 0"
                    },
                    {
                      "position": "relative"
                    },
                    {
                      "td": {
                        "width": "@nameWidth + @tablePad",
                        "padding-left": "@tablePad",
                        "activeText": {
                          "text": "#multiplayer/name",
                          "pare-text": "yes",
                          "width": "fw"
                        }
                      }
                    },
                    {
                      "td": {
                        "width": "0.5*(pw - 1@nameWidth - 2@tablePad)",
                        "activeText": {
                          "text": "#multiplayer/numPlayers",
                          "pare-text": "yes",
                          "width": "fw",
                          "halign": "center"
                        }
                      }
                    },
                    {
                      "td": {
                        "width": "(pw - 1@nameWidth - 2@tablePad)/2",
                        "activeText": {
                          "id": "sessions_diff_header",
                          "text": "#multiplayer/difficultyShort",
                          "pare-text": "yes",
                          "width": "fw",
                          "halign": "center"
                        }
                      }
                    }
                  ]
                }
              },
              {
                "table": {
                  "id": "sessions_table",
