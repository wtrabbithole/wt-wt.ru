                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "session_jip",
                      "smallFont": "yes"
                    }
                  }
                },
                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "session_cluster",
                      "smallFont": "yes"
                    }
                  }
                },
                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "disable_airfields",
                      "smallFont": "yes"
                    }
                  }
                },
                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "spawn_ai_tank_on_tank_maps",
                      "smallFont": "yes"
                    }
                  }
                },
                {
                  "tdiv": {
                    "id": "vehicles_info_button_block",
