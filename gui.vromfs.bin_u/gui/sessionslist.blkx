                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "limited_ammo",
                      "smallFont": "yes"
                    }
                  }
                },
                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "session_respawn",
                      "smallFont": "yes"
                    }
                  }
                },
                {
                  "tdiv": {
                    "pos": "pw-w, 0",
                    "position": "relative",
                    "textarea": {
                      "id": "session_takeoff",
