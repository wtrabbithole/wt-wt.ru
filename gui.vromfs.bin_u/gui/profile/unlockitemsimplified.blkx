{
  "selImg": {
    "mainDescWrap": {
      "padding": "@scrn_tgt_font/100.0",
      "padding-right": "1.5*@scrn_tgt_font/100.0",
      "width": "pw",
      "layeredIconContainer": {
        "id": "achivment_ico",
        "size": "0.5*@profileUnlockIconSize, 0.5*@profileUnlockIconSize",
        "position": "relative",
        "pos": "0, ph/2 - h/2",
        "decal_locked": "no",
        "achievement_locked": "no"
      },
      "achivmentTitle": [
        {
          "padding-top": "0.5@scrn_tgt_font/100.0"
        },
        {
          "margin-left": "1.5*@scrn_tgt_font/100.0"
        },
        {
          "min-height": "0.5*@profileUnlockIconSize"
        },
        {
          "width": "fw"
        },
        {
          "tinyFont": "yes"
        },
        {
          "flow": "vertical"
        },
        {
          "activeText": {
            "id": "achivment_title",
            "text": "",
            "max-width": "pw",
            "pare-text": "yes",
            "overlayTextColor": "unlockHeader"
          }
        },
        {
          "textarea": {
            "id": "description",
            "short-description": "yes",
            "width": "pw",
            "overflow": "hidden",
            "tinyFont": "yes",
            "style": "wrap-indent: 0; paragraph-indent: 0;",
            "text": ""
          }
        },
        {
          "challengeDescriptionProgress": {
            "id": "progress_bar",
            "value": "",
            "display": "hide",
            "style": "width:pw;"
          }
        },
        {
          "textarea": {
            "id": "reward",
            "text": "",
            "tinyFont": "yes",
            "margin-top": "1.15*@scrn_tgt_font/100.0",
            "style": "wrap-indent: 0; paragraph-indent: 0;",
            "max-width": "pw",
            "pare-text": "yes"
          }
        }
      ]
    },
    "closeBtn": {
      "id": "removeFromFavoritesBtn",
      "position": "absolute",
      "pos": "pw-w,ph-h*1.1",
      "on_click": "onRemoveUnlockFromFavorites",
      "style": "tooltip:#mainmenu/UnlockAchievementsRemoveFromFavorite/hint;",
      "unlockId": "null",
      "img": []
    }
  }
}