{
  "tdiv": [
    {
      "width": "pw"
    },
    {
      "pos": "0.5pw-0.5w, 0"
    },
    {
      "position": "relative"
    },
    {
      "height": "@bigOptionsHeight"
    },
    {
      "flow": "vertical"
    },
    {
      "overflow-y": "auto"
    },
    {
      "tdiv": [
        {
          "id": "facebook_frame"
        },
        {
          "width": "pw"
        },
        {
          "flow": "vertical"
        },
        {
          "tdiv": {
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "margin-bottom": "0.01@scrn_tgt_font",
            "img": {
              "width": "0.03pw",
              "height": "w",
              "max-width": "32*@sf/@pf",
              "max-height": "32*@sf/@pf",
              "pos": "0, 0.5ph-0.5h",
              "position": "relative",
              "background-image": "#ui/gameuiskin#facebook_logo"
            },
            "activeText": {
              "pos": "0.01@scrn_tgt_font, 0.5ph-0.5h",
              "position": "relative",
              "text": "#options/facebookTitle",
              "caption": "yes"
            },
            "Button_text": {
              "id": "facebook_login_btn",
              "text": "#options/facebookLogin",
              "tooltip": "#guiHints/facebookLogin",
              "hideText": "yes",
              "_on_click": "onFacebookLogin",
              "externalLink": "yes",
              "btnName": "start",
              "position": "relative",
              "pos": "0.01@scrn_tgt_font, 0",
              "ButtonImg": [],
              "activeText": {
                "text": "#options/facebookLogin",
                "pos": "0, 50%ph-50%h",
                "position": "relative",
                "tinyFont": "yes",
                "underline": []
              }
            }
          }
        },
        {
          "tdiv": [
            {
              "pos": "50%pw-50%w, 0"
            },
            {
              "position": "relative"
            },
            {
              "margin-bottom": "0.03@scrn_tgt_font"
            },
            {
              "Button_text": {
                "id": "facebook_like_btn",
                "tooltip": "#guiHints/facebookLike",
                "text": "#options/facebookLike",
                "_on_click": "onFacebookLike",
                "btnName": "RB",
                "visualStyle": "secondary",
                "buttonWink": [],
                "ButtonImg": []
              }
            },
            {
              "Button_text": {
                "id": "facebook_friends_btn",
                "padding-left": "0.01@scrn_tgt_font",
                "text": "#mainmenu/btnFacebookFriendsAdd",
                "_on_click": "onFacebookLoginAndAddFriends",
                "btnName": "Y",
                "visualStyle": "secondary",
                "buttonWink": [],
                "ButtonImg": []
              }
            }
          ]
        }
      ]
    },
    {
      "tdiv": [
        {
          "id": "twich_frame"
        },
        {
          "width": "pw"
        },
        {
          "flow": "vertical"
        },
        {
          "activeText": {
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "text": "#options/twitchStreaming",
            "caption": "yes",
            "margin-bottom": "0.01@scrn_tgt_font"
          }
        },
        {
          "optionsBoxRightScroll": {
            "id": "twitchOptions"
          }
        },
        {
          "tdiv": [
            {
              "id": "ttv_buttons_block"
            },
            {
              "width": "pw"
            },
            {
              "flow": "vertical"
            },
            {
              "tdiv": [
                {
                  "width": "pw"
                },
                {
                  "td": {
                    "cellType": "left",
                    "width": "0.5pw",
                    "optiontext": {
                      "text": "#options/ttv_shortcut"
                    }
                  }
                },
                {
                  "td": {
                    "cellType": "right",
                    "width": "0.5pw",
                    "padding-left": "@optPad",
                    "Button_text": {
                      "_on_click": "onAssignTwitchButton",
                      "tooltip": "#guiHints/ttv_shortcut",
                      "textarea": {
                        "id": "ttv_shortcut",
                        "voiceShortcut": "yes",
                        "pos": "0, 0.5ph-0.5h",
                        "position": "relative",
                        "min-width": "@buttonWidth-@buttonTextPadding",
                        "input-transparent": "yes",
                        "text-align": "center",
                        "text": ""
                      }
                    }
                  }
                }
              ]
            },
            {
              "tdiv": [
                {
                  "width": "pw"
                },
                {
                  "padding-top": "0.005@scrn_tgt_font"
                },
                {
                  "td": {
                    "cellType": "left",
                    "width": "0.5pw"
                  }
                },
                {
                  "td": {
                    "cellType": "right",
                    "width": "0.5pw",
                    "padding-left": "@optPad",
                    "Button_text": {
                      "_on_click": "onClearTwitchButton",
                      "text": "#mainmenu/btnReset",
                      "tooltip": "#mainmenu/btnReset"
                    }
                  }
                }
              ]
            }
          ]
        },
        {
          "tdiv": {
            "width": "pw",
            "padding-top": "0.005@scrn_tgt_font",
            "td": {
              "width": "pw",
              "height": "60"
            }
          }
        }
      ]
    }
  ]
}