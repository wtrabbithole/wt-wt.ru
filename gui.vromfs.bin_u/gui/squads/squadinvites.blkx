{
  "div": [
    {
      "size": "sw, sh"
    },
    {
      "pos": "0, 0"
    },
    {
      "position": "root"
    },
    {
      "behavior": "button"
    },
    {
      "behavior": "accesskey"
    },
    {
      "accessKey": "Esc | J:B"
    },
    {
      "on_click": "goBack"
    },
    {
      "on_r_click": "goBack"
    }
  ],
  "popup_menu": [
    {
      "id": "squad_invites"
    },
    {
      "position": "root"
    },
    {
      "not-input-transparent": "yes"
    },
    {
      "css-hier-invalidate": "yes"
    },
    {
      "flow": "vertical"
    },
    {
      "menu_align": "top"
    },
    {
      "div": {
        "id": "options_block",
        "width": "0.35@sf",
        "padding-bottom": "1@blockInterval",
        "flow": "vertical",
        "activeText": {
          "pos": "50%pw - 50%w, 0",
          "position": "relative",
          "text": "#options/squadSize",
          "text-align": "center"
        },
        "ComboBox": {
          "id": "squad_size_option",
          "size": "pw, @buttonHeight",
          "pos": "50%pw - 50%w, 1@blockInterval",
          "position": "relative",
          "btnName": "X",
          "on_select": "onSquadSizeChange"
        },
        "textAreaCentered": {
          "width": "pw",
          "pos": "50%pw - 50%w, 1@blockInterval",
          "position": "relative",
          "tinyFont": "yes",
          "text": "#guiHints/squadSize"
        }
      }
    },
    {
      "activeText": {
        "id": "invited_players_header",
        "pos": "50%pw - 50%w, 1@blockInterval",
        "position": "relative",
        "text": "#squad/invitedPlayers",
        "text-align": "center",
        "margin": "0.01@sf, 0"
      }
    },
    {
      "div": {
        "id": "invites_list",
        "pos": "50%pw-50%w, 0",
        "position": "relative",
        "padding-bottom": "1@blockInterval",
        "flow": "h-flow",
        "flow-align": "center",
        "behavior": "posNavigator",
        "navigatorShortcuts": "active",
        "childsActivate": "yes",
        "on_activate": "onInviteMemberMenu"
      }
    },
    {
      "popup_menu_arrow": []
    }
  ]
}