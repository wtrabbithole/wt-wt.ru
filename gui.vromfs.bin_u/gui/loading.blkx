                "position": "relative",
                "background-rotation": "0",
                "behavior": "increment",
                "inc-target": "background-rotation",
                "inc-factor": "160",
                "wait_icon_cock": []
              },
              "textAreaCentered": {
                "name": "loading",
                "style": "color:@white; font:@small;",
                "textShade": "yes",
                "text": "#loading",
                "pos": "5, 50%ph-50%h-2",
                "position": "relative"
              }
            }
          },
          "copyright": {
            "pos": "pw-w-1@bw, 0.5(ph-1@bh) - 50%h",
            "position": "absolute"
          }
        }
      },
      {
        "tdiv": [
          {
            "width": "0.67sw"
          },
          {
            "pos": "50%pw-50%w, 83%sh - 1@bh -50%h"
          },
          {
            "position": "absolute"
          },
          {
            "flow": "vertical"
          },
          {
            "background-color": "@shadeBackgroundColor4"
          },
          {
            "background-repeat": "expand"
          },
          {
            "padding": "0.005sw, 0.01sh, 0.005sw, 0.02sh"
          },
          {
            "textAreaCentered": {
              "id": "help_text",
              "text": "",
              "style": "color:@helpF1Color;",
