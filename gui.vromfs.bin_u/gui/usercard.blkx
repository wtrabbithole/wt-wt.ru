            }
          },
          {
            "animated_wait_icon": {
              "id": "profile_loading",
              "pos": "0.5(pw-w),0.4(ph-h)",
              "position": "absolute",
              "background-rotation": "0",
              "wait_icon_cock": []
            }
          },
          {
            "dummy": {
              "id": "profile_update",
              "behavior": "Timer",
              "timer_handler_func": "onUpdate"
            }
          }
        ]
      },
      {
        "tdiv": {
          "id": "stats-container",
          "size": "pw, fh",
          "margin-top": "1@blockInterval",
          "flow": "vertical",
          "HorizontalListBox": {
            "id": "modes_list",
            "height": "4*@scrn_tgt/100.0",
            "pos": "0, 0",
            "position": "relative",
            "activeAccesskeys": "LeftRight",
            "navigatorShortcuts": "yes",
            "on_wrap_up": "onWrapUp",
            "on_wrap_down": "onWrapDown",
            "on_select": "onStatsModeChange"
