                "on_click": "onBlacklistBan",
                "btnName": "RT",
                "ButtonImg": []
              }
            },
            {
              "Button_text": {
                "id": "btn_complain",
                "text": "#mainmenu/btnComplain",
                "btnName": "LT",
                "_on_click": "onComplain",
                "ButtonImg": []
              }
            }
          ],
          "navMiddle": {
            "id": "paginator_place"
          }
        }
      }
