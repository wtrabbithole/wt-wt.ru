                "margin": "0, 1@blockInterval",
                "behaviour": "wrapNavigator",
                "navigatorShortcuts": "yes",
                "childsActivate": "yes",
                "on_wrap_up": "onWrapUp",
                "on_wrap_down": "onWrapDown"
              }
            },
            {
              "tdiv": {
                "id": "units_boxes",
                "pos": "pw-w, 0",
                "position": "absolute",
                "margin": "0, 1@blockInterval",
                "class": "checkBoxRow",
                "behaviour": "wrapNavigator",
                "navigatorShortcuts": "yes",
                "childsActivate": "yes",
                "on_wrap_up": "onWrapUp",
                "on_wrap_down": "onWrapDown"
              }
            }
          ],
          "frameBlock": [
            {
              "size": "pw, fh"
            },
            {
              "position": "relative"
            },
            {
              "pos": "50%pw-50%w, 0"
            },
            {
              "position": "relative"
            },
            {
              "padding-top": "1@blockInterval"
            },
            {
              "table": {
                "id": "airs_stats_table",
                "size": "pw, ph",
                "pos": "50%pw-50%w, 0",
                "position": "relative",
                "class": "lbTable",
                "higlightRowOnHover": "yes",
                "text-valign": "center",
                "text-halign": "center"
              }
            }
          ],
          "animated_wait_icon": {
            "id": "stats_loading",
            "pos": "50%pw-50%w, 50%ph-50%h",
            "position": "absolute",
            "background-rotation": "0",
            "wait_icon_cock": []
          }
        }
      },
