{
  "root": {
    "background-color": "@modalShadeColor",
    "frame": [
      {
        "id": "countryChoice-root"
      },
      {
        "width": "0.75@scrn_tgt+2@framePadding"
      },
      {
        "pos": "50%pw-50%w, 45%ph-40%h"
      },
      {
        "position": "absolute"
      },
      {
        "class": "wnd"
      },
      {
        "frame_header": [
          {
            "activeText": {
              "caption": "yes",
              "text": "#mainmenu/btnTutorial"
            }
          },
          {
            "top_right_holder": {
              "CheckBox": {
                "id": "skip_tutorial",
                "pos": "0, 50%ph-50%h",
                "position": "relative",
                "text": "#options/doNotAskMeAgain",
                "tooltip": "#options/doNotAskMeAgain/tutorial/hint",
                "on_change_value": "onSkipTutorial",
                "btnName": "X",
                "CheckBoxImg": [],
                "ButtonImg": []
              }
            }
          },
          {
            "Button_close": {
              "id": "btn_close_tutorial",
              "on_click": "onClose",
              "userInputType": "mouse",
              "img": []
            }
          },
          {
            "dummy": {
              "on_click": "onClose",
              "accessKey": "Esc",
              "userInputType": "keyboard"
            }
          },
          {
            "dummy": {
              "on_click": "onClose",
              "accessKey": "J:B",
              "userInputType": "gamepad"
            }
          }
        ]
      },
      {
        "img": {
          "id": "tutorial_image",
          "width": "pw",
          "height": "400/800w",
          "max-width": "800",
          "max-height": "400",
          "pos": "50%pw-50%w, 0",
          "position": "relative",
          "background-image": "#ui/images/tutorial_fighter.jpg?P1",
          "background-color": "@white"
        }
      },
      {
        "textareaNoTab": {
          "id": "msgCaption",
          "pos": "50%pw-50%w, 0",
          "position": "relative",
          "text-align": "center",
          "max-width": "p.p.w-0.05@scrn_tgt",
          "caption": "yes",
          "text": ""
        }
      },
      {
        "textAreaCentered": {
          "id": "msgText",
          "width": "pw",
          "margin-bottom": "1.2@battleButtonHeight"
        }
      },
      {
        "navBar": {
          "style": "size:pw, 1.2@battleButtonHeight;",
          "navMiddle": {
            "Button_text": {
              "id": "btn_start",
              "userInputType": "mouse",
              "class": "battle",
              "text": "#multiplayer/btnStart",
              "on_click": "onStart",
              "btnName": "A",
              "style": "size:1@bigButtonWidth, 1@battleButtonHeight;",
              "buttonWink": {
                "_transp-timer": "0"
              },
              "ButtonImg": [],
              "btnText": {
                "text": "#multiplayer/btnStart"
              },
              "buttonGlance": []
            }
          }
        }
      },
      {
        "dummy": {
          "on_click": "onStart",
          "accessKey": "Space | Enter | NumEnter",
          "userInputType": "keyboard"
        }
      },
      {
        "dummy": {
          "on_click": "onStart",
          "accessKey": "J:A",
          "userInputType": "gamepad"
        }
      }
    ]
  }
}