{
  "css": "gui/css.css",
  "root": {
    "type": "big",
    "background-color": "@modalShadeColor",
    "frame": [
      {
        "width": "0.99sw $min 1.7sh"
      },
      {
        "pos": "0.5pw-0.5w, 0.05*@scrn_tgt"
      },
      {
        "position": "absolute"
      },
      {
        "class": "wnd"
      },
      {
        "flow": "vertical"
      },
      {
        "frame_header": {
          "flow": "vertical",
          "height": "2@frameHeaderHeight",
          "HorizontalListBox": {
            "id": "tabs_list",
            "height": "1@frameHeaderHeight",
            "class": "header",
            "smallFont": "yes",
            "withImages": "no",
            "activeAccesskeys": "RS",
            "on_select": "onHelpSheetChange"
          },
          "Button_close": {
            "img": []
          }
        }
      },
      {
        "HorizontalListBox": {
          "id": "sub_tabs_list",
          "height": "1@frameHeaderHeight",
          "smallFont": "yes",
          "withImages": "no",
          "activeAccesskeys": "LeftRight",
          "on_select": "onHelpSubSheetChange"
        }
      },
      {
        "tdiv": {
          "id": "help_sheet",
          "size": "pw, ph"
        }
      },
      {
        "tdiv": {
          "id": "link_lines_block",
          "size": "pw, ph",
          "position": "root"
        }
      }
    ]
  }
}