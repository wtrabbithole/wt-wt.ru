{
  "PhysSys": {
    "find": "^suspension_(._\\d\\d)$",
    "points": {
      "suspension": {
        "name": "suspension_$1"
      },
      "wheel": {
        "name": "bone_wheel_$1",
        "limitMin": [-1000.0,0.0,0.0],
        "limitMax": [1000.0,0.0,0.0]
      }
    },
    "constraints": {
      "edge": {
        "from": "suspension",
        "to": "wheel"
      }
    }
  }
}