        {
          "varModify": {
            "operator": "add",
            "value": 1.0,
            "var": "pa_units_quantity"
          }
        }
      ],
      "else_actions": [
        {
          "comment": {
            "value": "подсчитали количество юнитов"
          }
        },
        {
          "comment": {
            "value": "поделим число на 2"
          }
        },
        {
          "varModify": {
            "operator": "divAB",
            "value": 2.0,
            "var": "pa_units_quantity"
          }
        },
        {
          "comment": {
            "value": "получим количество текущих очков команды А"
          }
        },
        {
          "varSetInt": {
            "value": 0,
            "var": "pa_cur_tickets",
            "source_var": "mpTi1"
          }
        },
        {
          "varSetInt": {
            "value": 0,
            "var": "pa_tickets_cost"
          }
        },
        {
          "varModify": {
            "operator": "add",
            "value": 0.0,
            "var": "pa_tickets_cost",
            "use_variable": true,
            "var_value": "pa_cur_tickets"
          }
        },
        {
          "comment": {
            "value": "разделим количество очков на количество юнитов"
          }
        },
        {
          "varModify": {
            "operator": "divAB",
            "value": 1.0,
            "var": "pa_tickets_cost",
            "use_variable": true,
            "var_value": "pa_units_quantity"
          }
        },
        {
          "comment": {
            "value": "добавим 1 чтобы гарантировать нулевое количество очков при смерти всей наземки"
          }
        },
        {
          "varModify": {
            "operator": "add",
            "value": 1.0,
            "var": "pa_tickets_cost"
          }
        },
        {
          "comment": {
            "value": "умножаем число на -1, чтобы тикеты отнимались"
          }
        },
        {
          "varModify": {
            "operator": "mul",
            "value": -1.0,
            "var": "pa_tickets_cost"
          }
        },
        {
          "comment": {
            "value": "запускаем триггер окончания подсчетов"
          }
        },
        {
          "triggerEnable": {
            "target": "pa_ajust_tickets_end"
